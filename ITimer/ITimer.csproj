<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>RobIII</Authors>
    <Company>Devcorner.nl</Company>
    <Copyright>Copyright Â© 2020 Devcorner.nl</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/RobThree/ITimer</PackageProjectUrl>
    <RepositoryUrl>https://github.com/RobThree/ITimer</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>timer clock</PackageTags>
    <Version>1.0.0-alpha</Version>
    <Description>Provides a testable abstraction and alternative to System.Threading.Timer / System.Timers.Timer</Description>
    <PackageIcon>logo.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
